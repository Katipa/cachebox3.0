    <groundspeak:cache id="3645129" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Weinheimer Rhythmusakademie</groundspeak:name>
      <groundspeak:placed_by>Steilwand</groundspeak:placed_by>
