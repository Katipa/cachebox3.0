    <groundspeak:cache id="5524880" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Rumpelkiste</groundspeak:name>
      <groundspeak:placed_by>Sarah&amp;Basti</groundspeak:placed_by>
      <groundspeak:owner>Sarah&amp;Basti</groundspeak:owner>
