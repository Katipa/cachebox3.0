    <groundspeak:cache id="6141663" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Der Handschuh - Bonus</groundspeak:name>
      <groundspeak:placed_by>Teufelsprinzessin</groundspeak:placed_by>
      <groundspeak:owner>Teufelsprinzessin</groundspeak:owner>
