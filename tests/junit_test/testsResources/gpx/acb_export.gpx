    <groundspeak:cache id="5513843" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Zeit für etwas Malediktologie....</groundspeak:name>
      <groundspeak:placed_by>hutzy</groundspeak:placed_by>
