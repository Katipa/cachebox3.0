    <groundspeak:cache id="6457612" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>RevivalCache: Kurpfalz-Adventskalender 2006 Nr. 14</groundspeak:name>
      <groundspeak:placed_by>struwwelchen</groundspeak:placed_by>
