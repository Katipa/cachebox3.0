    <groundspeak:cache id="936610" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>UNESCO Geopark Bergstrasse Odenwald - Felsenmeer</groundspeak:name>
      <groundspeak:placed_by>Wolf64</groundspeak:placed_by>
      <groundspeak:owner>Wolf64</groundspeak:owner>
      <groundspeak:type>Earthcache</groundspeak:type>
