    <groundspeak:cache id="835724" available="True" archived="False" xmlns:groundspeak="http://www.groundspeak.com/cache/1/0/1">
      <groundspeak:name>Jugendstil in Karlsruhe - Oststadt</groundspeak:name>
      <groundspeak:placed_by>Bredoteau</groundspeak:placed_by>
